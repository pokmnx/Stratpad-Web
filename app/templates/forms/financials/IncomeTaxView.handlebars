<header>
	<hgroup>
		<h1>{{heading}}</h1>	
		<h2>{{it_title}}</h2>
	</hgroup>
</header>
<section>
	<article id="incomeTax">
		<div class="stratFileHelp">
			<iframe src="" data-url="help/view-financials/options/incomeTax/?iframe=1" width="100%" height="90%" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
		</div>

	    <div class="permissions"></div>		

		<div class="section">
			<h3>What income tax do you pay on your business' profits?</h3>
			<fieldset>
				<ul class="clean-list percentages">
					<li id="rate1">
						<div class="value">&nbsp;</div> 			
						<label>Rate on amounts up to: <span class="salaryLimit" contenteditable="true" data-key="salaryLimit1">0</span></label>
						<div class="slider" data-field="rate1"></div>
					</li>
					<li id="rate2">
						<div class="value">&nbsp;</div> 			
						<label>Rate on amounts up to: <span class="salaryLimit" contenteditable="true" data-key="salaryLimit2">0</span></label>
						<div class="slider" data-field="rate2"></div>
					</li>
					<li id="rate3">
						<div class="value">&nbsp;</div> 			
						<label>Rate on amounts over: <span class="salaryLimit" data-key="salaryLimit2">0</span></label>
						<div class="slider" data-field="rate3"></div>
					</li>
				</ul>
			</fieldset>
		</div>

		<div class="section">
			<h3>How many years can losses carry forward?</h3>
			<fieldset>
				<ul class="clean-list years">
					<li id="yearsCarryLossesForward">
						<div class="value">&nbsp;</div> 			
						<label>Years</label>
						<div class="slider" data-field="yearsCarryLossesForward"></div>
					</li>
				</ul>
			</fieldset>
		</div>

		<div class="section remittanceMonth">		
			<h3>In which month do you remit your income taxes?</h3>
			<fieldset>
		        <select name="remittanceMonth" id="remittanceMonth">
		          <option value="0">January</option>
		          <option value="1">February</option>
		          <option value="2">March</option>
		          <option value="3">April</option>
		          <option value="4">May</option>
		          <option value="5">June</option>
		          <option value="6">July</option>
		          <option value="7">August</option>
		          <option value="8">September</option>
		          <option value="9">October</option>
		          <option value="10">November</option>
		          <option value="11">December</option>
		        </select>
			</fieldset>
		</div>

	</article>
</section>



