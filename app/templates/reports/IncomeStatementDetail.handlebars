<header>
	<hgroup>
		<h1>{{heading_details}}</h1>	
		<h2>{{title}}</h2>
	</hgroup>
</header>

<section>
	<article id="incomeStatementDetail" class="group fullWidth">
		<div class="stratFileHelp">
			<iframe src="" data-url="help/view-financials/income-statement-monthly/?iframe=1" width="100%" height="90%" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
		</div>
		<div class="reportWrapper">		
			<div class="headerWrapper">
				<table id="tableRowHeaders" class="tableRowHeaders">
					<thead>
						<!-- empty cell at top of row headers -->
						<tr class="rowDivider2">
							<th>&nbsp;</th>
						</tr>
					</thead>
					<tbody>
						<!-- eg a single content row; must be entirely replaced.

						<tr>
							<td class="rowLevel1">Net Changes from Operations</td>
						</tr>
						-->
					</tbody>
				</table>
			</div>
			<div class="tableWrapper">
				<table id="fullReportTable" class="fullReportTable">
					<thead>
						<!-- 
							- column headers in content section
							- predicated on 8*12 + 1 columns = a fixed width for the table content 
							- must be entirely replaced

						<tr class="rowDivider2">
							<th>&nbsp;</th>
							<td>Jan 2012</td>
							<td>Feb 2012</td>
							<td>Mar 2012</td>
							<td>Apr 2012</td>
							<td>May 2012</td>
							<td>...</td>
						</tr>
						-->
					</thead>
					<tbody>
						<!-- 
							- must also have 8*12+1 columns 
							- # rows must match # rows in #tableRowHeaders 
							- we repeat the row header in order to ascertain the cell height
							- must be entirely replaced
						
						<tr>
							<td class="rowLevel1">Net Changes from Operations</td>
							<td>-999,999,999</td>
							<td>-999,999,999</td>
							<td>-999,999,999</td>
							<td>10,000</td>
							<td>10,000</td>
							<td>10,000</td>
						</tr>
						-->
					</tbody>
				</table>
			</div>
			<div id="disclaimer">
				{{fs_disclaimer}}
			</div>
		</div>
	</article>
</section>